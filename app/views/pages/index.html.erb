<%= "Pages" %>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Published At</th>
    <th>User</th>
    <th>Ancestry</th>
  </tr>
  <% for page in @pages %>
    <tr>
      <td><%= page.title %></td>
      <td><%= page.content %></td>
      <td><%= page.published_at %></td>
      <td><%= page.user_id %></td>
      <td><%= page.ancestry %></td>
      <td><%= link_to "Show", page %></td>
      <td><%= link_to "Edit", edit_page_path(page) %></td>
      <td><%= link_to "Destroy", page, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Page", new_page_path %></p>
